{
  "name": "quipuswap-farming",
  "version": "0.0.1",
  "license": "MIT",
  "description": "Quipuswap Farming",
  "scripts": {
    "postinstall": "patch-package",
    "start-sandbox": "docker run --rm --name my-sandbox -e flextesa_node_cors_origin='*' -e block_time=1 --detach -p 8732:20000 tqtezos/flextesa:20210602 granabox start",
    "clean": "rm -rf ./build",
    "compile": "yarn clean && ts-node scripts/cli.js compile -f json --c q_farm t_farm burner proxy_minter baker_registry",
    "migrate": "yarn compile && ts-node scripts/cli.js migrate",
    "migrate-granadanet": "yarn compile && ts-node scripts/cli.js migrate -n granadanet",
    "migrate-mainnet": "yarn compile && ts-node scripts/cli.js migrate -n mainnet",
    "test": "ts-mocha --paths test/*.spec.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/madfish-solutions/quipuswap-farming.git"
  },
  "dependencies": {
    "@taquito/michelson-encoder": "^10.1.0",
    "@taquito/rpc": "^10.1.0",
    "@taquito/signer": "^10.1.0",
    "@taquito/taquito": "^10.1.0",
    "@types/expect": "^24.3.0",
    "@types/mocha": "^9.0.0",
    "assert": "^2.0.0",
    "chai": "^4.3.4",
    "child_process": "^1.0.2",
    "dotenv": "^10.0.0",
    "fs": "^0.0.1-security",
    "ganache-cli": "^6.12.1-tezos.0",
    "mocha": "^9.0.1",
    "patch-package": "^6.4.7",
    "truffle": "^5.5.0-tezos.4",
    "ts-mocha": "^8.0.0",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.5",
    "yargs": "^17.0.1"
  },
  "bugs": {
    "url": "https://github.com/madfish-solutions/quipuswap-farming/issues"
  },
  "homepage": "https://github.com/madfish-solutions/quipuswap-farming#readme",
  "keywords": [
    "tezos",
    "ligo",
    "blockchain"
  ]
}
